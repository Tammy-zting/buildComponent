{
  "name": "AntdProLayout",
  "version": "1.0.0",
  "description": "绝影内置Layout",
  "private": true,
  "browser": "dist/main.js",
  "author": "xlj",
  "license": "MIT",
  "repository": {
    "type": "ssh",
    "url": "git@gitlabdev.paciolan.info:development/library/javascript/remote-component-starter.git"
  },
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --mode development",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack-dev-server.config.js",
    "clean": "rimraf dist",
    "cz": "git-cz",
    "test": "jest",
    "test:changed": "npm run test -- --changedSince HEAD",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint ."
  },
  "dependencies": {
    "antd": "^3.19.8",
    "babel-plugin-import": "^1.12.0",
    "classnames": "^2.2.6",
    "css-loader": "^3.0.0",
    "glob-all": "^3.1.0",
    "less-loader": "^5.0.0",
    "memoize-one": "^5.0.4",
    "omit.js": "^1.0.2",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-media-hook2": "^1.1.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "git-cz": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run build && npm run test:changed",
      "pre-push": "npm run test:coverage"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
